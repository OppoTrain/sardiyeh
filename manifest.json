{
    "name": "سرديّة",
    "version": "1.0.2",
    "description": "نروي السرديّة الفلسطينية",
    "manifest_version": 3,
    "author": "الشعب",
    "action":{
        "default_popup": "index.html",
        "default_title": "سرديّة",
        "default_icon": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
        }
    },
"permissions": [
        
        "storage"
    ],
    "content_scripts": [
        {
          "matches": ["https://*.google.com/*"],
          "js": ["content.js"],
          "run_at":"document_end"
        }
      ],
      "web_accessible_resources": [
    {
      "resources": ["images/Palestine_Flag.png"],
      "matches": ["https://*.google.com/*"] 
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
